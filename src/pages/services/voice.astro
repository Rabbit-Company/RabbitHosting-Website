---
import Layout from "../../layouts/Layout.astro";
import Navbar from "../../layouts/Navbar.astro";
import Footer from "../../layouts/Footer.astro";

import VoicesLayout from "../../layouts/Voices.astro";
import Voice from "../../layouts/Voice.astro";
---

<Layout title="Rabbit Hosting - Voices">
	<Navbar active="voices" />
	<VoicesLayout>
		<Voice
			title="LavaLink Hosting"
			path="/voice/lavalink"
			image="/images/services/lavalink.svg"
		/>
		<Voice
			title="Team Speak 3 Hosting"
			path="/voice/teamspeak"
			image="/images/services/TeamSpeak-logo.png"
			rounded
		/>
		<Voice
			title="TS3 Manager Hosting"
			path="/voice/teamspeakmanager"
			image="/images/services/TeamSpeak-logo.png"
		/>
		<Voice
			title="Mumble Server Hosting"
			path="/voice/mumble"
			image="/images/services/1200px-Icons_mumble.svg.png"
			rounded
		/>
		<Voice
			title="TeaSpeak Hosting"
			path="/voice/teaspeak"
			image="/images/services/teaspeak.png"
			rounded
		/>
	</VoicesLayout>
	<Footer />
	<script is:inline src="/js/default.js"></script>
</Layout>
