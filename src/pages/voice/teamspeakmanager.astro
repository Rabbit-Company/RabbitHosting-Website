---
import Layout from "../../layouts/Layout.astro";
import Navbar from "../../layouts/Navbar.astro";
import Footer from "../../layouts/Footer.astro";

import ServicePreview from "../../layouts/ServicePreview.astro";
import Packages from "../../layouts/Packages.astro";
import Package from "../../layouts/Package.astro";
import EmptyComparison from "../../layouts/EmptyComparison.astro";

const sleepRabbit = [
	"1GB RAM",
	"15GB Nvme Storage",
	"0.25 dedicated Ryzen 9 7950X3D thread",
	"1 database included",
	"1 backup included",
	"Pterodactyl panel",
];
const fastRabbit = [
	"3GB RAM",
	"45GB Nvme Storage",
	"0.75 dedicated Ryzen 9 7950X3D thread",
	"1 database included",
	"1 backup included",
	"Pterodactyl panel",
];
const crazyRabbit = [
	"5GB RAM",
	"75GB Nvme Storage",
	"1.25 dedicated Ryzen 9 7950X3D thread",
	"2 database included",
	"2 backup included",
	"Pterodactyl panel",
];
---

<Layout
	title="Rabbit Hosting - TeamSpeak Managment Server Hosting"
	trustpilot
	sellix
>
	<Navbar active="teamspeakmanager" />

	<ServicePreview
		title="TeamSpeak Managment Server Hosting"
		description="The Open Source Webinterface For TeamSpeak Servers"
		image="/images/services/teamspeak3m.svg"
	/>

	<Packages>
		<Package
			title="Sleepy Rabbit"
			description="For small-sized TeamSpeak servers."
			price="1.50"
			productID="64cf13ffbaec7"
			template={sleepRabbit}
		/>
		<Package
			title="Fast Rabbit"
			description="For medium-sized TeamSpeak servers."
			price="4.50"
			productID="64cf149c0e781"
			template={fastRabbit}
			popular
		/>
		<Package
			title="Crazy Rabbit"
			description="For large-sized TeamSpeak servers."
			price="7.50"
			productID="64cf14f413c6d"
			template={crazyRabbit}
		/>
	</Packages>

	<EmptyComparison />

	<Footer />
	<script is:inline src="/js/default.js"></script>
</Layout>
